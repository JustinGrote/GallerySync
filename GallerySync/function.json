{
  "bindings": [
    {
      "name": "Timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "Checkpoint",
      "type": "blob",
      "direction": "in",
      "path": "source/checkpoint",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "CheckpointOut",
      "type": "blob",
      "direction": "out",
      "path": "source/checkpoint",
      "connection": "AzureWebJobsStorage"
    }
  ]
}